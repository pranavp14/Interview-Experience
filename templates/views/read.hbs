<!DOCTYPE html>
<html lang="en">
{{>header}}
<body>
    {{>navbar}}
      {{#each data}}
     <div class="col-lg-4" data-aos="fade-up" data-aos-delay="200">
            <div class="post-box">  
              <h3 class="post-title"><strong></strong>{{CompanyName}} Interview Experience for {{jobRole}} ({{campus}})</strong></h3>
              <div class="meta">
                <span class="post-date">{{placeDate}}</span>
                <span class="post-author"> / {{StudentName}}</span>
              </div>
              {{#if round1}}
                   <strong>Round 1({{round1}}):</strong>
                   <p>{{ experience1}}</p>
              {{/if}}
              {{#if round2}}
                   <strong>Round 2({{round2}}):</strong>
                   <p>{{ experience2}}</p>
              {{/if}}
              {{#if round3}}
                   <strong>Round 3({{round3}}):</strong>
                   <p>{{ experience3}}</p>
              {{/if}}
              {{#if round3}}
                   <strong>Round 3({{round3}}):</strong>
                   <p>{{ experience3}}</p>
              {{/if}}
              {{#if round4}}
                   <strong>Round 4({{round4}}):</strong>
                   <p>{{ experience4}}</p>
              {{/if}}
               {{#if round5}}
                   <strong>Round 5({{round5}}):</strong>
                   <p>{{ experience5}}</p>
              {{/if}}
              {{#if message}}
                   <strong>Messages:</strong>
                   <p>{{message}}</p>
              {{/if}}
              {{#if source}}
                   <strong>Resources:</strong>
                   <p>{{source}}</p>
              {{/if}}
              
              {{!-- <a href="blog-details.html" class="readmore stretched-link"><span>Read More</span><i class="bi bi-arrow-right"></i></a> --}}
            </div>
          </div>

    {{/each}}
</body>
</html>